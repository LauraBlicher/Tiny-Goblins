		void surf( Input i , inout SurfaceOutputStandard o )
		{
			o.Normal = float3(0,0,1);
			float3 ase_worldPos = i.worldPos;
			float3 ase_worldNormal = WorldNormalVector( i, float3( 0, 0, 1 ) );
			float4 triplanar25 = TriplanarSamplingSF( _TopTexture0, ase_worldPos, ase_worldNormal, 1.0, float2( 1,1 ), 1.0, 0 );
			float3 ase_vertexNormal = mul( unity_WorldToObject, float4( ase_worldNormal, 0 ) );
			float4 triplanar29 = TriplanarSamplingSF( _TopTexture2, ase_worldPos, ase_worldNormal, 1.0, float2( 1,1 ), 1.0, 0 );
			float4 triplanar31 = TriplanarSamplingSF( _TopTexture3, ase_worldPos, ase_worldNormal, 1.0, float2( 1,1 ), 1.0, 0 );
			o.Emission = ( ( triplanar25 * abs( ase_vertexNormal.x ) ) + ( triplanar29 * abs( ase_vertexNormal.y ) ) + ( abs( ase_vertexNormal.z ) * triplanar31 ) ).xyz;
