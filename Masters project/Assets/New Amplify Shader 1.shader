		};

		uniform sampler2D _NormalSomething;
		uniform float4 _NormalSomething_ST;
		uniform sampler2D _TextureSample1;
		uniform float4 _TextureSample1_ST;
		uniform float _Float0;

		void surf( Input i , inout SurfaceOutputStandard o )
		{
			float2 uv_NormalSomething = i.uv_texcoord * _NormalSomething_ST.xy + _NormalSomething_ST.zw;
